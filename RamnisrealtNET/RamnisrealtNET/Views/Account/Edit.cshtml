@model RamnisrealtNET.ViewModels.EditUserViewModel
@{
    ViewBag.Title = "Редактирование пользователя";
}
<form asp-action="Edit" asp-controller="Account">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <input type="hidden" asp-for="Id" />
    </div>
    <div class="form-group">
        <label asp-for="F" class="control-label">Фамилия</label>
        <input type="text" asp-for="F" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="I" class="control-label">Имя</label>
        <input type="text" asp-for="I" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="O" class="control-label">Отчество</label>
        <input type="text" asp-for="O" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Phon" class="control-label">Телефон</label>
        <input type="text" asp-for="Phon" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="City" class="control-label">Город</label>
        <input type="text" asp-for="City" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Adres" class="control-label">Адрес</label>
        <input type="text" asp-for="Adres" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Pic" class="control-label">Изображение</label>
        <input type="text" asp-for="Pic" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-outline-secondary" />
    </div>
</form>