@using RamunisrealtNET.Models
@model IEnumerable<Contracts>
@{
    ViewData["Title"] = "Удалить?";
}

<div class="modal modal-sheet position-static d-block bg-secondary py-5" tabindex="-1" role="dialog" id="modalSheet">
    <div class="modal-dialog" role="document">
        <div class="modal-content rounded-6 shadow">
            <div class="modal-header border-bottom-0">
                <h5 class="modal-title">Delete object?</h5>
                <a href="/Home/Property"><button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button></a>
</div>
            <div class="modal-body py-0">
                <p>Add № @ViewBag.Numer will be deleted.</p>
            </div>
            <div class="modal-footer flex-column border-top-0">
                    <form method="post">
                    <button type="submit" class="btn btn-lg btn-primary w-100 mx-0 mb-2">Save changes</button>
                    </form>
                    <a href="/Home/Property"><button type="button" class="btn btn-lg btn-light w-100 mx-0" data-bs-dismiss="modal">Close</button></a>
            </div>
        </div>
    </div>
</div>